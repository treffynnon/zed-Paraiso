            "selection": "#ef61553d"
          },
          {
            "cursor": "#fec418",
            "background": "#fec418",
            "selection": "#fec4183d"
          },
          {
            "cursor": "#5bc4bf",
            "background": "#5bc4bf",
            "selection": "#5bc4bf3d"
          },
          {
            "cursor": "#e7e9db",
            "background": "#e7e9db",
            "selection": "#e7e9db3d"
          }
