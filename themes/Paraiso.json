{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "author": "Simon Holywell (based on a theme by Jan T. Sott)",
  "name": "Paraíso",
  "themes": [
    {
      "appearance": "dark",
      "name": "Paraíso Dark",
      "style": {
        // Chrome colours
        "background": "#2f1e2e",
        "elevated_surface.background": "#2f1e2e",
        // the panel colours are used in the right hand pane of the editor for the file list etc
        "panel.background": "#41323f",
        "panel.focused_border": "#2f1e2e",
        "tab_bar.background": "#41323f",
        "tab.active_background": "#2f1e2e",
        "tab.inactive_background": "#41323f",
        // title bar of the entire application
        "title_bar.background": "#2f1e2e",
        // this is the background colour of panels at the top of the editor like search and replace etc
        "toolbar.background": "#2f1e2e",
        "scrollbar.track.background": "#2f1e2e",
        "scrollbar.thumb.background": "#4f424c88",
        "scrollbar.thumb.hover_background": "#4f424cFF",
        "scrollbar.thumb.border": "#4f424c",
        // this is the bar at the bottom of the editor
        "status_bar.background": "#2f1e2e",

        // Editor pane colours
        "editor.active_line.background": "#41323f",
        "editor.active_line_number": "#a39e9b",
        "editor.background": "#2f1e2e",
        "editor.document_highlight.read_background": "#4f424c",
        "editor.document_highlight.write_background": "#4f424c",
        "editor.foreground": "#a39e9b",
        "editor.gutter.background": "#2f1e2e",
        "editor.highlighted_line.background": "#4f424c",
        "editor.invisible": "#776e71",
        "editor.line_number": "#776e71",
        "editor.active_wrap_guide": "#a39e9b",
        "editor.wrap_guide": "#776e71",
        "editor.subheader.background": "#41323f",

        // Misc styles
        "link_text.hover": "#06b6ef",
        "drop_target.background": "#815ba445",
        "search.match_background": "#fec418",
        "pane.focused_border": "#815ba4",
        "surface.background": "#2f1e2e",

        // Border colours
        "border": "#41323f",
        "border.disabled": "#776e71",
        "border.selected": "#815ba4",
        "border.focused": "#fec418",
        "border.variant": "#815ba4",
        "border.transparent": null,

        // Icon colours
        "icon": "#a39e9b",
        "icon.accent": "#815ba4",
        "icon.disabled": "#776e71",
        "icon.muted": "#8e847e",
        "icon.placeholder": "#776e71",

        // Text colours
        "text": "#a39e9b",
        "text.accent": "#815ba4",
        "text.disabled": "#776e71",
        "text.muted": "#8e847e",
        "text.placeholder": "#776e71",

        // Element colours
        "element.background": "#41323f",
        "element.selected": "#2f1e2e",
        "element.hover": "#4f424c",
        "element.active": "#41323f",
        "element.disabled": "#776e71",

        "ghost_element.background": "#2f1e2e",
        "ghost_element.selected": "#41323f",
        "ghost_element.hover": "#4f424c",
        "ghost_element.active": "#41323f",
        "ghost_element.disabled": "#776e71",

        // Colours
        "error": "#ef6155",
        "error.background": "#2f1e2e",
        "error.border": "#ef6155",
        "hint": "#5bc4bf",
        "hint.background": "#2f1e2e",
        "hint.border": "#5bc4bf",
        "info": "#06b6ef",
        "info.border": "#06b6ef",
        "info.background": "#2f1e2e",
        "predictive": "#5bc4bf",
        "predictive.background": "#2f1e2e",
        "predictive.border": "#5bc4bf",
        "renamed": "#fec418",
        "renamed.background": "#2f1e2e",
        "renamed.border": "#fec418",
        "success": "#48b685",
        "success.background": "#2f1e2e",
        "success.border": "#48b685",
        "unreachable": "#776e71",
        "unreachable.background": "#2f1e2e",
        "unreachable.border": "#776e71",
        "warning": "#f99b15",
        "warning.background": "#2f1e2e",
        "warning.border": "#f99b15",

        // Git colours
        "conflict": "#ef6155",
        "conflict.background": "#2f1e2e",
        "conflict.border": "#ef6155",
        "created": "#48b685",
        "created.background": "#2f1e2e",
        "created.border": "#48b685",
        "deleted": "#ef6155",
        "deleted.background": "#2f1e2e",
        "deleted.border": "#ef6155",
        "hidden": "#776e71",
        "hidden.background": "#2f1e2e",
        "hidden.border": "#776e71",
        "ignored": "#776e71",
        "ignored.background": "#2f1e2e",
        "ignored.border": "#776e71",
        "modified": "#815ba4",
