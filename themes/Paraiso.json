            "cursor": "#a39e9b",
            "background": "#41323f",
            "selection": "#4f424c"
          },
          {
            "cursor": "#06b6ef",
            "background": "#06b6ef",
            "selection": "#06b6ef3d"
          },
          {
            "cursor": "#48b685",
            "background": "#48b685",
            "selection": "#48b6853d"
          },
          {
            "cursor": "#815ba4",
            "background": "#815ba4",
            "selection": "#815ba43d"
          },
          {
            "cursor": "#ef6155",
